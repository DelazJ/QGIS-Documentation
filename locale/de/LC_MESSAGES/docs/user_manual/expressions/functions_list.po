msgstr "``map_akeys(map('1','one','2','two'))`` → [ '1', '2' ]"

#: ../../docs/user_manual/expressions/expression_help/Maps.rst:95
msgid "map_avals"
msgstr "map_avals"

#: ../../docs/user_manual/expressions/expression_help/Maps.rst:97
msgid "Returns all the values of a map as an array."
msgstr "Gibt alle Werte einer Zuordnung als Array zurück."

#: ../../docs/user_manual/expressions/expression_help/Maps.rst:103
msgid "map_avals(map)"
msgstr "map_avals(map)"

