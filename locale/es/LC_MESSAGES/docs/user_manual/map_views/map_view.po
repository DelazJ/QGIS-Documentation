msgid ""
"|radioButtonOff| :guilabel:`Synchronize view to selection`: same as zoom to "
"selection"
msgstr ""
"|radioButtonOff| :guilabel:`Sincronizar vista a selección`: igual que el "
"zoom a la selección"

#: ../../docs/user_manual/map_views/map_view.rst:1444
msgid "radioButtonOff"
msgstr "radioButtonOff"

#: ../../docs/user_manual/map_views/map_view.rst:1262
msgid ":guilabel:`Scale`"
msgstr ":guilabel:`Escala`"

#: ../../docs/user_manual/map_views/map_view.rst:1263
msgid ":guilabel:`Rotation`"
msgstr ":guilabel:`Rotación`"

#: ../../docs/user_manual/map_views/map_view.rst:1264
msgid ":guilabel:`Magnification`"
msgstr ":guilabel:`Magnificación`"

