#: ../../docs/user_manual/introduction/general_tools.rst:2395
msgid "hideAllLayers"
msgstr "hideAllLayers"

#: ../../docs/user_manual/introduction/general_tools.rst:128
msgid "|showSelectedLayers| :guilabel:`Show Selected Layers`"
msgstr "|showSelectedLayers| :guilabel:`Rodyti pažymėtus sluoksnius`"

#: ../../docs/user_manual/introduction/general_tools.rst:2497
msgid "showSelectedLayers"
msgstr "showSelectedLayers"

#: ../../docs/user_manual/introduction/general_tools.rst:129
msgid "|hideSelectedLayers| :guilabel:`Hide Selected Layers`"
msgstr "|hideSelectedLayers| :guilabel:`Slėpti pažymėtus sluoksnius`"

#: ../../docs/user_manual/introduction/general_tools.rst:2399
msgid "hideSelectedLayers"
